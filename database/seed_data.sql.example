INSERT INTO article (id, url, published_at, title, description, source, categories, category, daily_summary_id) VALUES (9421, 'https://tvn24.pl/kultura-i-styl/wielki-marty-w-kinie-ten-film-powstal-dla-chalameta-st8874162?source=rss', '2026-01-30 19:50:13.000000', '"Wielki Marty" to nie tylko "wielki" Chalamet', '"Wielki Marty" należy do rzadko występujących opowieści kinowych. Takich, w których wszystko się zgadza. Film powstał z myślą o Timothee Chalamecie, który w tytułowej roli ostatecznie udowadnia swój ogromny talent. Dziewięć oscarowych nominacji nie jest tu żadnym zaskoczeniem. Przyjrzeliśmy się fenomenowi "Wielkiego Marty''ego" z trzech perspektyw.', 'TVN24', '["najnowsze"]', 'Culture', 21);
