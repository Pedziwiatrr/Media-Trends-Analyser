{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky",
    "lint": "eslint --max-warnings 0",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "spellcheck": "cspell \"**/*.{ts,tsx,md,json}\"",
    "knip": "knip",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "validate": "npm run lint && npm run knip && npm run spellcheck && npm run test",
    "check": " npm run prettier:check && npm run validate && npm run build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "npx eslint --fix --max-warnings 0",
    "*.{js,jsx,ts,tsx,css,md,json}": "npx prettier --write"
  },
  "dependencies": {
    "next": "16.0.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "recharts": "^3.5.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.25",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "cspell": "^9.3.2",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "knip": "^5.69.1",
    "lint-staged": "^16.2.6",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5.9.3"
  }
}
